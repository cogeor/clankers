<?xml version="1.0"?>
<robot name="quadruped">
    <!-- 3-DOF quadruped: 4 legs × 3 joints (hip_ab + hip_pitch + knee_pitch) = 12 DOF
         Body: 0.4m × 0.2m × 0.1m box, 5kg
         Legs: hip_link + 0.15m upper + 0.15m lower + foot sphere
         Total mass: 9.0 kg (body 5 + 4 × 1.0 per leg)
         Per-leg chain: body → hip_ab (X) → hip_link → hip_pitch (Y) → upper_leg
                        → knee_pitch (Y) → lower_leg → foot_fixed → foot
         Hip positions relative to body center:
           FL: (+0.15, +0.08, -0.05)  FR: (+0.15, -0.08, -0.05)
           RL: (-0.15, +0.08, -0.05)  RR: (-0.15, -0.08, -0.05) -->

    <!-- ============ BODY ============ -->
    <link name="body">
        <inertial>
            <mass value="5.0"/>
            <inertia ixx="0.02083" ixy="0" ixz="0"
                     iyy="0.07083" iyz="0" izz="0.08333"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.4 0.2 0.1"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <box size="0.4 0.2 0.1"/>
            </geometry>
        </collision>
    </link>

    <!-- ============ FRONT LEFT LEG ============ -->
    <link name="fl_hip_link">
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.00003" ixy="0" ixz="0" iyy="0.00003" iyz="0" izz="0.00003"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.04 0.04 0.04"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <box size="0.04 0.04 0.04"/>
            </geometry>
        </collision>
    </link>

    <link name="fl_upper_leg">
        <inertial>
            <origin xyz="0 0 -0.075"/>
            <mass value="0.5"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.0002"/>
        </inertial>
        <visual>
            <origin xyz="0 0 -0.075"/>
            <geometry>
                <cylinder radius="0.015" length="0.15"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 -0.075"/>
            <geometry>
                <cylinder radius="0.015" length="0.15"/>
            </geometry>
        </collision>
    </link>

    <link name="fl_lower_leg">
        <inertial>
            <origin xyz="0 0 -0.075"/>
            <mass value="0.3"/>
            <inertia ixx="0.0006" ixy="0" ixz="0" iyy="0.0006" iyz="0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin xyz="0 0 -0.075"/>
            <geometry>
                <cylinder radius="0.012" length="0.15"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 -0.075"/>
            <geometry>
                <cylinder radius="0.012" length="0.15"/>
            </geometry>
        </collision>
    </link>

    <link name="fl_foot">
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001"/>
        </inertial>
        <visual>
            <geometry>
                <sphere radius="0.02"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.02"/>
            </geometry>
        </collision>
    </link>

    <joint name="fl_hip_ab" type="revolute">
        <parent link="body"/>
        <child link="fl_hip_link"/>
        <origin xyz="0.15 0.08 -0.05"/>
        <axis xyz="1 0 0"/>
        <limit lower="-0.5" upper="0.5" effort="20" velocity="10"/>
        <dynamics damping="0.1" friction="0.05"/>
    </joint>

    <joint name="fl_hip_pitch" type="revolute">
        <parent link="fl_hip_link"/>
        <child link="fl_upper_leg"/>
        <origin xyz="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-1.0" upper="1.0" effort="30" velocity="10"/>
        <dynamics damping="0.1" friction="0.05"/>
    </joint>

    <joint name="fl_knee_pitch" type="revolute">
        <parent link="fl_upper_leg"/>
        <child link="fl_lower_leg"/>
        <origin xyz="0 0 -0.15"/>
        <axis xyz="0 1 0"/>
        <limit lower="-2.5" upper="0.0" effort="30" velocity="10"/>
        <dynamics damping="0.1" friction="0.05"/>
    </joint>

    <joint name="fl_foot_fixed" type="fixed">
        <parent link="fl_lower_leg"/>
        <child link="fl_foot"/>
        <origin xyz="0 0 -0.15"/>
    </joint>

